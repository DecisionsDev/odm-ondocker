<security-constraint>
	<web-resource-collection>
		<web-resource-name>Decision Service</web-resource-name>
		<url-pattern>/*</url-pattern>
	</web-resource-collection>
	<auth-constraint>
		<role-name>resExecutors</role-name>
	</auth-constraint>
</security-constraint>

<login-config>
	<auth-method>BASIC</auth-method>
	<realm-name>Decision Service</realm-name>
</login-config>
