<dc-usermanagement>
    <ldapConnections>
        <ldapConnection name="Test SCIM">
            <ldapUrl>http://keycloak-keycloak.apps.kanab.cp.fyre.ibm.com/auth/realms/master/scim/v2</ldapUrl>
            <searchConnectionDN>jdoe</searchConnectionDN>
            <searchConnectionPassword>jdoe</searchConnectionPassword>
            <groupSearchFilter>displayName%20co%20%22odm%22</groupSearchFilter>
            <groupNameAttribute>displayName</groupNameAttribute>
            <userIdAttribute>userName</userIdAttribute>
            <userNameAttribute>userName</userNameAttribute>
            <userMailAttribute>emails.value</userMailAttribute>
            <ldapProperties>
		<ldapProperty>
			<ldapPropertyName>ScimAuthenticationKind</ldapPropertyName>
			<ldapPropertyValue>OAUTH</ldapPropertyValue>
		</ldapProperty>
            </ldapProperties>
	    <ldapProperties>
		<ldapProperty>
		    <ldapPropertyName>ScimAuthenticationProvider</ldapPropertyName>
		    <ldapPropertyValue>keycloack_pwd</ldapPropertyValue>
		</ldapProperty>
            </ldapProperties>
            <ldapProperties>
		<ldapProperty>
			<ldapPropertyName>IsScim</ldapPropertyName>
			<ldapPropertyValue>true</ldapPropertyValue>
		</ldapProperty>
            </ldapProperties> 
        </ldapConnection>
    </ldapConnections>
</dc-usermanagement>
